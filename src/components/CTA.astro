---
import Button from './Button.astro';
import roadmap from '../icons/roadmap.png';
import tokenomics from '../icons/tokenomics.png';
---

<section class="grid-lg cta">
  <section class="programmer container blur grid-sm">
    <img src={roadmap} alt="Roadmap" />
  </section>
  <section class="programmer container blur grid-sm">
    <img src={tokenomics} alt="Tokenomics" />
  </section>
  <div class="client-logo-container">
    <Button width="150" text="Twitter" link="https://twitter.com/sbc_r2e/" style="secondary" />
    <Button width="150" text="Group Chat" link="https://t.me/superbicycleclubchat/" style="secondary" />
    <Button width="150" text="Email" link="mailto:admin@superbicycle.club" style="secondary" />
  </div>
  <div class="grid-sm">
    <small class="text-muted">Join US</small>
  </div>
</section>